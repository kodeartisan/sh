MYSQL_SERVER="mariadb"
MYSQL_VERSION="10.10"

# Securing MySQL installation.
MYSQL_SECURE_INSTALL=true

# Leave it blank for auto generated secure password.
MYSQL_ROOT_PASSWORD=""

# Allow remote client access.
MYSQL_ALLOW_REMOTE=false

# MariaDB backup user.
MARIABACKUP_USER="backupdb"

# Leave it blank for auto generated secure password.
MARIABACKUP_PASS=""